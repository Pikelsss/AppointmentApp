<div id="<%= dom_id task %>">
  <p class="my-5">
    <strong class="block font-medium mb-1">Item:</strong>
    <%= task.item %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Completed:</strong>
    <%= task.completed %>
  </p>

  <p class="my-5">
    <strong class="block font-medium mb-1">Appointment:</strong>
    <%= task.appointment_id %>
  </p>

</div>
